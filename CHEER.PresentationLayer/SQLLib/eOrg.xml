<?xml version="1.0" encoding="utf-8" ?> 
<SQLStringContainer>
	<eOrg.CreateBLOBFile>INSERT INTO ORGBLOBFILE (BLOBFILEID,VALUE,FILETYPE) VALUES(@BLOBFILEID,@VALUE,@FILETYPE)</eOrg.CreateBLOBFile>
	<eOrg.UpdateBLOBFile>UPDATE ORGBLOBFILE SET VALUE = @VALUE,FILETYPE = @FILETYPE Where BLOBFILEID = @BLOBFILEID</eOrg.UpdateBLOBFile>
	<eOrg.DeleteBLOBFile>DELETE FROM ORGBLOBFILE WHERE BLOBFILEID = @BLOBFILEID</eOrg.DeleteBLOBFile>
	<eOrg.SelectBLOBFile>SELECT * FROM ORGBLOBFILE WHERE BLOBFILEID = @BLOBFILEID</eOrg.SelectBLOBFile>
</SQLStringContainer>